public class GenericPrinter implements Printer1 {
        public void print(String msg){
          System.out.println("I am in method Printer of class GenericPrinter");
        }
}

class Printer {
	static void u(Obluda b) { b.ohrozit();  }
	static void v(NebezpecnaObluda d) {
		d.ohrozit();
		d.znicit();
	}
	public static void main(String[] args){
		Drak if2 = new Drak();
