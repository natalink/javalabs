package cviceni2;

//import java.util.Date;

interface Printer1 { void print(String msg);  }

class GenericPrinter implements Printer1 {
        public void print(String msg){
          System.out.println("I am in method Printer of class GenericPrinter");
        }
}

class Printer {
	static void u(Printer1 b) { b.print("Pokus neco vytisknout");  }

	public static void main(String[] args){
		GenericPrinter if2 = new GenericPrinter();
		u(if2);
//		v(if2);
		
	}
}



